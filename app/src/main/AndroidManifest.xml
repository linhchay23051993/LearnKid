<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.linhchay.learnkid">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/rate_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/rate_icon"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".activity.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.LearnActivity" />
        <activity android:name=".activity.MainActivity" />
        <activity android:name=".activity.PracticeActivity" />
        <activity android:name=".activity.InterviewActivity" />
    </application>

</manifest>